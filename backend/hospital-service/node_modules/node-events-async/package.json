{
  "name": "node-events-async",
  "description": "Async NodeJS EventEmitter",
  "version": "1.2.0",
  "author": "Panates",
  "license": "MIT",
  "dependencies": {
    "putil-promisify": "^1.10.1",
    "tslib": "^2.8.1"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./types/index.d.cts",
        "default": "./cjs/index.js"
      },
      "default": "./esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/eventsasync.git"
  },
  "engines": {
    "node": ">=16.0"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "events",
    "async",
    "EventEmitter",
    "AsyncEventEmitter"
  ]
}