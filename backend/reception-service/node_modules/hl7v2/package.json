{
  "name": "hl7v2",
  "description": "HL7 v2 parser, serializer, validator for NodeJS",
  "version": "1.4.4",
  "author": "Panates",
  "license": "MIT",
  "dependencies": {
    "@jsopen/objects": "^2.0.2",
    "iconv-lite": "^0.7.0",
    "is-typedarray": "^1.0.0",
    "node-events-async": "^1.2.0",
    "ts-gems": "^3.11.3",
    "tslib": "^2.8.1",
    "valgen": "^5.17.1",
    "uid": "^2.0.2"
  },
  "peerDependencies": {
    "hl7v2-dictionary": "^1.4.4"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./types/index.d.cts",
        "default": "./cjs/index.js"
      },
      "default": "./esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/hl7v2.git",
    "directory": "packages/common"
  },
  "engines": {
    "node": ">=18.0"
  },
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "hl7",
    "perser",
    "serializer"
  ],
  "publishConfig": {
    "access": "public"
  }
}